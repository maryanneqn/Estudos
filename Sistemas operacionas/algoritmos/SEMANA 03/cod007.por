programa {
  cadeia nome
  inteiro q                        // nome é o nome do item e q é a quantidade
  funcao inicio() {
    escreva("Digite o nome do item: ")
    leia(nome)
    escreva("Qual a quantidade? ")
    leia(q)
    escreva("Nome do item: ", nome, "\n")
    escreva("Quantidade:", q, "\n")
    escreva("Item: ", nome, ", Quantidade em estoque: ", q)

    
  }
}
